{
  "name": "bootstrap-design-sandbox",
  "version": "1.0.0",
  "description": "🅱️ 🏖️ Container for designing pages with Bootstrap",
  "main": "src/index.js",
  "repository": "https://github.com/cygnusb2b/bootstrap-design-sandbox",
  "author": "Jacob Bare <jacob@limit0.io>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prestart": "$npm_package_docker_install",
    "start": "$npm_package_docker_up",
    "stop": "$npm_package_docker_down",
    "terminal": "docker exec -it bootstrapdesignsandbox_app_1 /bin/bash"
  },
  "docker": {
    "install": "docker-compose run --no-deps --entrypoint yarn app",
    "up": "docker-compose up",
    "down": "docker-compose down"
  },
  "devDependencies": {
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "express": "^4.16.3"
  }
}
